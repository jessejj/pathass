<!DOCTYPE html>
<head>

    <title>Triage Cassettes</title>
 
	<HTA:APPLICATION
	APPLICATIONNAME = "Jesse's first HTA HTML Application"
	ID = "NAME"
        SINGLEINSTANCE = "Yes"
        SCROLL="no"
        BORDER="thin"
        INNERBORDER="no"
        MAXIMIZEBUTTON="no"
        
    />

</head>

<script language="VBScript">
    Sub Window_onLoad
        window.resizeTo 400,300
    End Sub
    
    Sub TriageCassettes
	    Set objShell = CreateObject("wscript.Shell")
		objShell.run "cscript.exe TriageCassettes.vbs"
	    Set objShell = Nothing
    End Sub

	Sub StopScript
	    Set objShell = CreateObject("wscript.Shell")
		objShell.run "taskkill.exe /F /IM cscript.exe /T"
		objShell.regwrite "HKEY_LOCAL_MACHINE\SOFTWARE\Wow6432Node\IMPAC\PseudoDriver\CIV-GRO-CAS1\v1.0\Directory Path", "\\CLPATHIF01\DIS_SHARE", "REG_SZ"
	    Set objShell = Nothing
	End Sub

</script>

<body style="font-family:'Segoe UI', Roboto,'Helvetica Neue',Arial,'Noto Sans',sans-serif" style="text-align:center">
<p>Hello world! </p>

<button type="button" onclick=TriageCassettes id="TestButton" name="TestButton">Triage Cassettes</button>
&nbsp;
<button type="button" onclick=StopScript id="TestButton" name="TestButton">Stop</button>

</body>
 
</html>
